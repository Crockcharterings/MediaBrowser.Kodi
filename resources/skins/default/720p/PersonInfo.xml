		<control type="textbox" id="3001">
			<description>text</description>
			<left>30</left>
			<top>60</top>
			<width>550</width>
			<height>100</height>
			<align>left</align>
			<label>-</label>
			<font>font13</font>
			<autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
		</control>

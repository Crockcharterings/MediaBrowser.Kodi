<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">8</defaultcontrol>
    <zorder>2</zorder>
	<coordinates>
		<system>1</system>
		<left>290</left>
		<top>120</top>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<control type="image">
			<left>0</left>
			<top>0</top>
			<width>700</width>
			<height>500</height>
			<texture border="40">DialogBack.png</texture>
		</control>
		<control type="image">
			<description>Dialog Header image</description>
			<left>40</left>
			<top>16</top>
			<width>630</width>
			<height>40</height>
			<texture>dialogheader.png</texture>
		</control>
